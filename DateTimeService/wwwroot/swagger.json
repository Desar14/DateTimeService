  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "payload_interval": {
        "properties": {
          "address_id": {
            "type": "string",
            "description": "Код СОАТО адреса",
            "example": "47175"
          },
          "delivery_type": {
            "type": "string",
            "description": "Вид доставки - самовывоз (self) или курьер (courier). Если не указан, то считается как доставка",
            "example": "self"
          },
          "floor": {
            "type": "number",
            "description": "Этаж",
            "example": 8
          },
          "payment": {
            "type": "string",
            "description": "Тип оплаты: partly_pay, other",
            "example": "other"
          },
          "order_items": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/orderitem"
            },
            "description": "Список товаров",
            "example": [
              {
                "code": "454100",
                "sales_code": "2770262",
                "quantity": 1
              },
              {
                "code": "424941",
                "quantity": 2
              },
              {
                "code": "609204",
                "quantity": 1
              },
              {
                "code": "6082000",
                "quantity": 6
              }
            ]
          }
        }
      },
      "payload_availabledate": {
        "properties": {
          "city_id": {
            "type": "string",
            "description": "Код города",
            "example": "17030"
          },
          "delivery_types": {
            "type": "array",
            "description": "Вид доставки - самовывоз (self) или курьер (courier)",
            "items": {
              "type": "string"
            },
            "example": [
              "self",
              "courier"
            ]
          },
          "codes": {
            "properties": {
              "code": {
                "type": "string",
                "description": "Артикул номенклатуры"
              },
              "sales_code": {
                "type": "string",
                "description": "Код номенклатуры для уценки (goods_id). Если не уценка, то не отправляется в принципе, либо null"
              }
            },
            "description": "Список артикулов с кодами уценки",
            "example": [
              {
                "code": "789734",
                "sales_code": "2242670"
              },
              {
                "code": "69516"
              }
            ]
          }
        }
      },
      "orderitem": {
        "properties": {
          "code": {
